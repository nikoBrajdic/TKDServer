        xmlns:Model="clr-namespace:TKD.App.Models"
        x:Class="TKD.App.MainWindow"
        mc:Ignorable="d"
        Title="Tournament Data"
        Height="591"
        Width="1082"
        Loaded="Window_Loaded"
        Closed="TournamentDataWindow_Closed"
        Icon="../img/icon.png"
        x:Name="TournamentWindow">
    <Window.Resources>
        <CollectionViewSource x:Key="poomsaeViewSource"
                              d:DesignSource="{d:DesignInstance {x:Type Model:Poomsae}, CreateList=True}" />
        <CollectionViewSource x:Key="contestantViewSource"
                              d:DesignSource="{d:DesignInstance {x:Type Model:Contestant}, CreateList=True}" />
        <CollectionViewSource x:Key="categoryViewSource"
                              d:DesignSource="{d:DesignInstance {x:Type Model:Category}, CreateList=True}" />
        <CollectionViewSource x:Key="teamViewSource"
                              d:DesignSource="{d:DesignInstance {x:Type Model:Team}, CreateList=True}" />
        <CollectionViewSource x:Key="poomsaeTypeViewSource"
                              d:DesignSource="{d:DesignInstance {x:Type Model:PoomsaeType}, CreateList=True}" />
        <CollectionViewSource x:Key="scoreViewSource"
                              d:DesignSource="{d:DesignInstance {x:Type Model:Score}, CreateList=True}" />
        
        
        <Style x:Key="VerticallyAlignCell"
               TargetType="{x:Type DataGridCell}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type DataGridCell}">
                        <Grid Background="{TemplateBinding Background}">
                            <ContentPresenter VerticalAlignment="Center" 
                                              HorizontalAlignment="Center" />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <DataTrigger Binding="{Binding Path=Content.Text, RelativeSource={RelativeSource Self}}"
                             Value="">
                    <Setter Property="Background"
                            Value="LightGray" />
                </DataTrigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="CenterGridHeaderStyle"
               TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment"
                    Value="Center" />
        </Style>

        <ControlTemplate x:Key="CurrCategoryTemplate" 
                         TargetType="DataGrid">
            <DataGrid CanUserAddRows="False"
                      CanUserDeleteRows="False"
                      CanUserResizeRows="False"
                      IsReadOnly="True"
                      CellStyle="{StaticResource VerticallyAlignCell}"
                      AutoGenerateColumns="False"
                      EnableRowVirtualization="True"
                      RowDetailsVisibilityMode="VisibleWhenSelected"
                      RowHeight="27"
                      MouseDoubleClick="OpenContestant">
                <DataGrid.ContextMenu>
                    <ContextMenu>
                        <MenuItem Header="Next round"
                                  Click="ToNextRound">
                            <MenuItem.Icon>
                                <Image Source="../img/right-arrow.png" />
                            </MenuItem.Icon>
                        </MenuItem>
                        <MenuItem Header="Previous round"
                                  Click="ToPreviousRound">
                            <MenuItem.Icon>
                                <Image Source="../img/left-arrow.png" />
                            </MenuItem.Icon>
                        </MenuItem>
                    </ContextMenu>
                </DataGrid.ContextMenu>
                <DataGrid.Columns>
                    <DataGridTextColumn x:Name="Round1NameColumn" Binding="{Binding Path=Contestant.Name}" Header="Name" Width="110" />
                    <DataGridTextColumn x:Name="surnameColumn1" Binding="{Binding Path=Contestant.Surname}" Header="Surname" Width="110" />
                    <DataGridTextColumn x:Name="teamColumn1" Binding="{Binding Path=Contestant.Team}" Header="Team" Width="110" />
                    <DataGridTextColumn x:Name="MinorMeanColumn" Binding="{Binding Path=Score.MinorMean}" Header="Avg" Width="SizeToHeader" />
                    <DataGridTextColumn x:Name="GrandTotalColumn" Binding="{Binding Path=Score.GrandTotal}" Header="Grand Total" Width="SizeToHeader" />
                    <DataGridTextColumn x:Name="MinorTotalColumn" Binding="{Binding Path=Score.MinorTotal}" Header="All" Width="SizeToHeader" />
                    <DataGridTextColumn x:Name="AccuracyAverageColumn" Binding="{Binding Path=Score.AccuracyMinorTotal}" Header="A Avg" Width="50" />
                    <DataGridTextColumn x:Name="AccuracyTotalColumn" Binding="{Binding Path=Score.AccuracyGrandTotal}" Header="A All" Width="50" />
                    <DataGridTextColumn x:Name="Accuracy1Column" Binding="{Binding Path=Score.Accuracy1}" Header="A1" Width="25" />
                    <DataGridTextColumn x:Name="Accuracy2Column" Binding="{Binding Path=Score.Accuracy2}" Header="A2" Width="25" />
                    <DataGridTextColumn x:Name="Accuracy3Column" Binding="{Binding Path=Score.Accuracy3}" Header="A3" Width="25" />
                    <DataGridTextColumn x:Name="Accuracy4Column" Binding="{Binding Path=Score.Accuracy4}" Header="A4" Width="25" />
                    <DataGridTextColumn x:Name="Accuracy5Column" Binding="{Binding Path=Score.Accuracy5}" Header="A5" Width="25" />
